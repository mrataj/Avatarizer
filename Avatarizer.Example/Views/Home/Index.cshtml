@{
  ViewBag.Title = "Avatarizer";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

@section head
{
  <link rel="stylesheet" type="text/css" href="@Url.Content("~/Css/home.css")" />
}

<div class="subtext">
  <div>This is an example to see how Avatarizer generates user avatars based on their initials.</div>
  <div>You can see that same initials always get same avatar color.</div>
</div>

@for (int i = 0; i < 209; i++)
{
  <img src="@Url.Action("Avatar", "Users", new { id = i })" />
}